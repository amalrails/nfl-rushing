<p id="notice"><%= notice %></p>

<div class="container-fluid">
<%= search_form_for @q, remote: true , url: rushings_path, :method => :get do |f| %>
    <div class="col-md-2">
      <table class="search table table-striped table-hover table-bordered">
        <tr>
          <td>
            <%= f.text_field :player_cont, placeholder: 'Name contains', class: 'form-control' %>
            <%= f.submit class: 'btn btn-primary' %>
            <%= link_to "Clear", request.path, class: "btn btn-default" %>
          </td>
        </tr>
      </table>
    </div>
    <div id="rushings"><%= render 'rushings' %></div>
  <% end %>
</div>
